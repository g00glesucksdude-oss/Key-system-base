-- Stage 1: Load the b64 library
local b64url = "https://raw.githubusercontent.com/g00glesucksdude-oss/Complicated-key-system/main/b64"
local b64src = game:HttpGet(b64url)
local b64fn = loadstring(b64src)
local b64 = b64fn()  -- if the file returns a table
-- If it doesnâ€™t return, but defines globals, just call b64_encode / b64_decode directly

print("Base64 loaded")

-- Quick sanity check
if b64 and b64.encode and b64.decode then
    local test = "Hello"
    local enc = b64.encode(test)
    local dec = b64.decode(enc)
    print("Encoded:", enc)
    print("Decoded:", dec)
end

-- Stage 2: Load your dependent module
local moduleUrl = "https://raw.githubusercontent.com/g00glesucksdude-oss/Complicated-key-system/main/yourmodule"
local moduleSrc = game:HttpGet(moduleUrl)
local moduleFn = loadstring(moduleSrc)
local module = moduleFn()

print("Module loaded:", typeof(module))
