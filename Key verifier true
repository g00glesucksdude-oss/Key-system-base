-- Load the raw file directly
local url = "https://raw.githubusercontent.com/g00glesucksdude-oss/Complicated-key-system/main/b64"
local src = game:HttpGet(url)

-- Wrap it so it always returns a table
local wrapped = src .. [[

return {
    encode = encode or b64_encode,
    decode = decode or b64_decode
}]]

local ok, b64 = pcall(loadstring(wrapped))
if not ok then
    warn("‚ùå Failed to load b64:", b64)
    return
end

-- Test
local test = "Hello, Roblox!"
local enc = b64.encode(test)
local dec = b64.decode(enc)

print("Encoded:", enc)
print("Decoded:", dec)
